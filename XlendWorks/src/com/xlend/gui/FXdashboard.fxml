<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" prefHeight="-1.0" prefWidth="-1.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="com.xlend.gui.FXdashboardController">
  <children>
    <BorderPane layoutX="0.0" layoutY="0.0" prefHeight="600.0" prefWidth="800.0">
      <bottom>
        <VBox fx:id="taskBar" prefHeight="200.0" prefWidth="100.0">
          <children>
            <HBox fx:id="taskBar1" alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
              <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" fx:id="x1" />
              </padding>
              <VBox.margin>
                <Insets left="10.0" right="10.0" top="10.0" fx:id="x2" />
              </VBox.margin>
            </HBox>
            <HBox fx:id="taskBar2" alignment="TOP_CENTER" padding="$x1" prefHeight="100.0" prefWidth="200.0" spacing="20.0" VBox.margin="$x2" />
          </children>
        </VBox>
      </bottom>
      <top>
        <BorderPane fx:id="upperPane" padding="$x1" prefHeight="200.0" prefWidth="200.0">
          <top>
            <HBox minHeight="20.0" prefHeight="20.0" prefWidth="778.0">
              <children>
                <Label fx:id="userLogin" alignment="CENTER" contentDisplay="CENTER" prefWidth="775.0" text="Hi" textFill="WHITE">
                  <font>
                    <Font size="18.0" />
                  </font>
                </Label>
              </children>
            </HBox>
          </top>
        </BorderPane>
      </top>
    </BorderPane>
  </children>
  <stylesheets>
    <URL value="@fxdashboard.css" />
  </stylesheets>
</AnchorPane>
